{
  "name": "tourze/wechat-work-media-bundle",
  "description": "媒体资源服务",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/collections": "^2.3",
    "doctrine/dbal": "^3.7.0 || ^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^2.20 || ^3.0",
    "doctrine/persistence": "^3.1 || ^4",
    "symfony/http-foundation": "^6.4",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "tourze/enum-extra": "0.0.*",
    "tourze/easy-admin-attribute": "0.0.*",
    "tourze/http-client-bundle": "0.0.*",
    "tourze/doctrine-snowflake-bundle": "0.0.*",
    "tourze/wechat-work-bundle": "0.0.*",
    "tourze/json-rpc-core": "^0.0.6",
    "tourze/symfony-temp-file-bundle": "0.0.*",
    "tourze/json-rpc-log-bundle": "0.0.*",
    "tourze/json-rpc-lock-bundle": "0.0.*",
    "psr/simple-cache": "^1.0|^2.0|^3.0",
    "yiisoft/json": "^1.0",
    "league/flysystem": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "WechatWorkMediaBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WechatWorkMediaBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}